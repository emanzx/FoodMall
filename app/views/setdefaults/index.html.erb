<p id="notice"><%= notice %></p>

<h1>Setdefaults</h1>

<table>
  <thead>
    <tr>
      <th>Chef</th>
      <th>Runner</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setdefaults.each do |setdefault| %>
      <tr>
        <td><%= setdefault.chef %></td>
        <td><%= setdefault.runner %></td>
        <td><%= setdefault.user %></td>
        <td><%= link_to 'Show', setdefault %></td>
        <td><%= link_to 'Edit', edit_setdefault_path(setdefault) %></td>
        <td><%= link_to 'Destroy', setdefault, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setdefault', new_setdefault_path %>
